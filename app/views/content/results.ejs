<div class="container">
    <h1><%=title %></h1>
    <div class="row">
        <div class="column">
            <table class="table table-striped table-bordered table-hover">
                <thead class="table-dark">
                <tr>
                    <th scope="col" class="text-center">#</th>
                    <th scope="col">Topic</th>
                    <th scope="col">Question 1</th>
                    <th scope="col"></th>
                    <th scope="col"></th>
                    <th scope="col"></th>
                </tr>
                </thead>
                <tbody id="resultsList">
                <% for(let count=0; count < answers.length; count++) { %>
                    <tr>
                        <% for(let count=0; count < answers.length; count++) { %>
                        <td scope="row" class="text-center"></td>
                        <td><%= survey[count].topic %></td>
                        <td><%= survey[count].question1 %></td>
                            <td><%= answers[count].answer1 %></td>
                            <td><%= answers[count].answer2 %></td>
                            <td><%= answers[count].answer3 %></td>
                            <% } %>
                    </tr>
                <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>
