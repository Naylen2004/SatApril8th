<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Survey Search Results</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="./content/form.css">
</head>
<body>
    <%- include('../../components/header.ejs') %>
    
    <h1>Survey Search Results</h1>
    <div class="container">
        <form class="d-flex" method="GET" action="/searchsurvey">
            <input class="form-control me-2" type="search" placeholder="Search Survey" aria-label="Search" name="q">
            <button class="btn btn-outline-success" type="submit">Search</button>
        </form>
    </div>
    <% if (surveys.length > 0) { %>
        <ul>
            <% surveys.forEach(survey => { %>
                <li>
                    <a href="/viewsurvey?quiz=<%= survey.quiz %>"><%= survey.quiz %></a>
                </li>
            <% }); %>
        </ul>
    <% } else { %>
        <p>No surveys found.</p>
    <% } %>
</body>
</html>
